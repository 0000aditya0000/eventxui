{
  "expo": {
    "name": "EventX",
    "slug": "ems",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.knoldussoftware.ems"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.knoldussoftware.ems",
      "permissions": ["DETECT_SCREEN_CAPTURE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "28fcaf2b-d4f0-4a05-a5ba-5eda83cbd680"
      }
    },
    "owner": "knoldussoftware",
    "plugins": ["expo-secure-store"]
  }
}
